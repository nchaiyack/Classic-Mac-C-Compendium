Clib.o "{MPW}"Libraries:Libraries:SIOW.o 		"{Libraries}"Runtime.o "{Libraries}"Interface.o  		libdf.a  libvg.a  vcompat.c.o 		-o vcompat		# compile and make the sample vset test file vtest1	C -d MAC  -d MPW -mc68020 -mc68881 -mbg full -t -r -sym full -opt full vtest1.c	Link -d -c '????' -t APPL 		"{CLibraries}"StdClib.o "{MPW}"Libraries:Libraries:SIOW.o 		"{Libraries}"Runtime.o "{Libraries}"Interface.o  		libdf.a  libvg.a  vtest1.c.o 		-o vtest1		# compile and make the sample vset test file vtesthi	C -d MAC  -d MPW -mc68020 -mc68881 -mbg full -t -r -sym full -opt full vtesthi.c	Link -d -c '????' -t APPL 		"{CLibraries}"StdClib.o "{MPW}"Libraries:Libraries:SIOW.o 		"{Libraries}"Runtime.o "{Libraries}"Interface.o  		libdf.a  libvg.a  vtesthi.c.o 		-o vtesthivtesthi	Fortran eghi.f -MC68881 -opt=1 -bkg=0	Link -d -c '????' -t APPL 		"{CLibraries}"StdClib.o 		"{Libraries}"Runtime.o "{Libraries}"Interface.o  		"{FLibraries}FORTRANLib.o" 		df.a  dff.a libvg.a  eghi.f.o 		-o eghi