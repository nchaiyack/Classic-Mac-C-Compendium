##############################	BigMake for DataScope#############################Echo "#	BigMakeLCsi begins"#If "`Exists -f :DSLibs:`"	Echo	"#### :DSLibs: exists as a file, not a folder"	Echo	"#### BigMakeLCsi terminating ... fview cannot be built"	Echo	"#"	Echo	"#"	ExitEndIf Not "`Exists -d :DSLibs:`"	Echo	"#	:DSLibs: folder created to hold object files and libraries"	Newfolder	:DSLibs:EndIf Not "`Exists -f :DSLibs:df.a`"	Duplicate	-y	:HDFpartial:df.a	:DSLibs:df.aEnd#Directory :DSIncludes:Echo "Directory `Directory`"BuildProgram ::newversion	-d DATE="`date -d -s`"BuildProgram fview.hBuildProgram macfview.hDirectory ::Echo "Directory `Directory`"#Directory :TCPIncludes:Echo "Directory `Directory`"BuildProgram macwhatami.hBuildProgram whatami.hBuildProgram protocol.hBuildProgram data.hDirectory ::Echo "Directory `Directory`"#Directory :DSResources:Echo "Directory `Directory`"Make -f MakeFileLCsiDirectory ::Echo "Directory `Directory`"#Directory :DSSource:Echo "Directory `Directory`"Make -f MakeFileLCsi ::DSLibs:f2view.c.oMake -f MakeFileLCsi ::DSLibs:fview.c.oMake -f MakeFileLCsi ::DSLibs:fparse.y.c.oMake -f MakeFileLCsi ::DSLibs:txtlib.oMake -f MakeFileLCsi ::DSLibs:cmplib.oDirectory ::Echo "Directory `Directory`"#Directory :TCPSource:Echo "Directory `Directory`"Make -f MakeFileLCsi ::DSLibs:tcplibd.oDirectory ::Echo "Directory `Directory`"#Echo "Make -f MakeFileLCsi"Echo "#"#