#set exit 1set olddir "``pwd``"set exit 1set toDir scratch-1:csm:if {#} == 0	choose -u speck -askpw "RUNET:Gorm Appleshare:speck" -type AFPServer	set from speck:	set outWithIt 'eject speck:'else	set from "{{1}}"	set outWithIt ''endcd "{{from}}"set ff "`f comp.Å`"cd "{olddir}"for n in {ff}	set exit 0	mkdir {toDir}{n}:	set exit 1	echo -n > {toDir}{n}:z	rm -y {toDir}{n}:Å	cp "{{from}}"{n} {toDir}{n}:articlesendcd "{olddir}"{outWithIt}