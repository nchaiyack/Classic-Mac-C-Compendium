# MacApp Startup쩭psset MASeparateObjectsPrefix ""set MALoadFiles "{ShellDirectory}Load_Files:"SET MABuildDefaults "{{MABuildDefaults}} -ModelFar -Names -CPlus -w2 -CPlus -warnings -CPlus off -NoSymantec -SCpp -O -SCpp none -SCpp -S -SCpp -w- -RenameFlag S7NmDbDmUnPeRaTeSmfCfD			'SymDebug' -RenameFlag S7NmDbDmUnPeRaTefCfD			'Debug' -RenameFlag S7NmTefCfD						'NoDebug' -RenameFlag S7FpNmTefCfD					'NoDebugFPU' "Set MADone 'Beep 2D,15,128'Set MAFailed 'Beep d,20,255'set MATemporaries "{CPlusScratch}"set MASCpp `which SCpp` ; export MASCppset OldMALibraries {MALibraries}; export OldMALibrariesset OldMALoadFiles {MALoadFiles}; export OldMALoadFilesif !{HasRamDisk}	exitendset exit 0flushcp -n {MATools}MABuild {MATools}MABuildTool {MATools}CreatorAndBundle {RamTools}cp -n {SCbin}SCpp {RamTools}set exit 0backup -r -a -c -from {CPlusIncludes} -to {TmpDir}CPlusIncludes: > {tmp2}set exit 1{tmp2}set CPlusIncludes {TmpDir}CPlusIncludes:set MALoadFiles {TmpDir}MALoadFiles:set MALibraries {TmpDir}MacAppLib:CopyMacAppLibs Debug###########