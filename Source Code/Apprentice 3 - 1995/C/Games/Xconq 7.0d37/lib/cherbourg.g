(game-module "cherbourg"  (title "Fall of Cherbourg")  (base-module "ww2-bn")  (variants   (see-all false)   (sequential true)   )  (instructions (   "As the Allies (actually just the US), you have just cut off the Cotentin peninsula. "   "Your goal is now to capture Cherbourg as soon as possible; in 7 days the Germans "   "will have rendered the port unusable."   ""   "As the Germans, your goal is to hold back the Americans during that time; "   "all your units may be sacrificed if necessary."   ))  )(scorekeeper (do last-side-wins));; This will cause units to focus appropriately.(add u* point-value 1)(add hq-inf-corps point-value 100);(scorekeeper;  (who 1);  (do (if (occupied 28 108) (win 1) (lose 2)));  )(set initial-date "8 19 Jun 1944");; The game lasts one week.(set last-turn 21)(add u* start-with 0)(side 1 (name "Allies") (adjective "Allied") (class "allied")   (color "blue") (emblem-name "white-star"))(side 2 (name "Germany") (adjective "German") (class "german")   (color "black") (emblem-name "german-cross"))(set sides-min 2)(set sides-max 2)(area 54 45 (restrict 186 159 12 104))(include "t-normandy") ; has to go here so towns get on German side(unit-defaults (@ 0 104));; Towns in Allied hands.(unit "Barneville" (s 1))(unit "Carteret" (s 1))(unit "Quineville" (s 1))(unit "St Mere-Eglise" (s 1))(unit "St Sauveur" (s 1));; Make this vanish - still German, but irrelevant here.(unit "La Haye-du-Puits" (hp 0));;; Allies consist of basically three American divisions.(inf-mot 24 18 1)(inf-mot 26 17 1)(inf-mot 27 17 1)(inf-mot 28 16 1)(inf-mot 30 16 1)(inf-mot 31 15 1)(inf-mot 32 15 1)(inf-mot 34 14 1)(inf-mot 35 14 1)(inf-mot 36 14 1)(inf-mot 39 12 1)(inf-mot 40 11 1)(inf-mot 42 11 1)(inf-mot 43 11 1)(inf-mot 44 11 1)(inf-mot 54 11 1)(inf-mot 42 10 1)(inf-mot 45 10 1)(inf-mot 46 10 1)(inf-mot 47 10 1)(inf-mot 48 10 1)(inf-mot 49 10 1)(inf-mot 50 10 1)(inf-mot 51 10 1)(inf-mo