package Rinaldi;&MacPerl'LoadExternals("Align 2.7 (US)");&MacPerl'LoadExternals("ArchiveContent 1.1 (US)");&MacPerl'LoadExternals("AtalkZones 1.5 (US)");# Calendoid doesn't work (yet), because it opens an external window.&MacPerl'LoadExternals("CapsLock 1.0 (US)");&MacPerl'LoadExternals("ChooseColor 1.1 (US)");&MacPerl'LoadExternals("Chooser 1.7 (US)");&MacPerl'LoadExternals("ClipInfo 1.0 (US)");&MacPerl'LoadExternals("CopyFolder 2.2 (US)");package main;print &Rinaldi'Align("line,'em\nup,neatly", "10,C"), "\n";print &Rinaldi'ArchiveContent("knuth:Development:MacPerl:Demo:CptTst.cpt"), "\n";print &Rinaldi'ATalkZones(), "\n";print &Rinaldi'CapsLock(), "\n";print &Rinaldi'ChooseColor("What's your favorite color baby?"), "\n";print &Rinaldi'CurrPrinter(), "\n";print &Rinaldi'ClipInfo(), "\n";# print &Rinaldi'CopyFolder("merlin:QuicKeys Scriptª", "merlin:QKS");print join("\n", keys _Rinaldi), "\n";