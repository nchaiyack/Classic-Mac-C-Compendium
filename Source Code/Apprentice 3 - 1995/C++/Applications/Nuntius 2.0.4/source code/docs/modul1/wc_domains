clear ¥:° "{{worksheet}}"set domains HD2:kilde:Nuntius:docs:domainstarget {domains}set domain_sum 0find ¥loop	find /?+/ {domains}	break if {status}	set theline "`cat "{domains}.¤"`"	if `ev "{{theline}}" =~ /[ ¶t]+¨1¶#Å/`		echo "{{theline}}"		continue	end	if `ev "{{theline}}" =~ /(Å[Â ¶t])¨1[ ¶t]*¶#Å/`		set theline "{{¨1}}"	end	if !`ev "{{theline}}" =~ /¶t(Å)¨1/`		if {domain_sum}			ev pages = ( {domain_sum} + 25 ) div 60			echo "¶t¶tSum: {domain_sum}   ~ {pages} sider"			set domain_sum 0		end		echo "{{theline}}"		continue	end	set m1 1	set d1 1	if `ev "{{theline}}" =~ /(Å)¨1 +([0-9]+)¨2¶/([0-9]+)¨3/`		set theline "{{¨1}}"		set m1 {¨2}		set d1 {¨3}	end	if `ev "{{theline}}" =~ /¶t(Å)¨1.hcp/`		set c1 `count -l {¨1}.h`		set c2 `count -l {¨1}.cp`		ev c = ( ( {c1} + {c2} ) * {m1} ) div {d1}		echo "¶t{c}¶t{¨1}.hcp"		ev domain_sum += {c}	else if `ev "{{theline}}" =~ /¶t([-a-z_.]+)¨1/`		set c `count -l {¨1}`		ev c = ( {c} * {m1} ) div {d1}		echo "¶t{c}¶t{¨1}"		ev domain_sum += {c}	endend ·· "{{worksheet}}"